  <!-- 'on' state for main/secondary navigation -->
  
  <script language="javascript" type="text/javascript">
		$(document).ready(function() {
		// Get rid of "http://"
		loc = location.href.substring(7);
		
		// Get rid of domain name
		loc = loc.substring(loc.indexOf("/"));
		
		// Get rid of fragment identifier
		var i = loc.indexOf("#");
		if (i > -1) loc = loc.substring(0, i);
		
		// Highlight navigator links to this page
		$(".nav a[href='" + loc + "']").addClass('selected');
		$(".secondarynav a[href='" + loc + "']").addClass('selected');
		});

	</script>
	
