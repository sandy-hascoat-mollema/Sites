  <head>
    
    <meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    
	<!-- Responsive and mobile friendly stuff -->
	<meta name="HandheldFriendly" content="True">
	<meta name="MobileOptimized" content="320">
	<meta name="viewport" content="width=device-width, target-densitydpi=160dpi, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->

    {squarespace-headers}
	
	<link rel="stylesheet" href="/styles/site.css" media="all">
	

    <squarespace:script src="site.js" combo="true" />
  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    
    
     <!-- Begin layout wrapper -->
    
    <div id="wrapper">
    
    <!--BEGIN HEADER-->
    
    
    <div id="header_wrap">
		<header id="header" class="clearfix">
		
		<!--SUB NAVIGATION-->
       
         <squarespace:navigation navigationId="secondaryNav" template="secondarynavigation" />
		
			<!--SITE TITLE OR LOGO-->

              {.section website}
        
                <div class="logo">
				<h1 data-content-field="site-title">
                  <a href="/">
                    {.section logoImageUrl}
                     <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
                    {.or}
                      {siteTitle}
                    {.end}
                  </a>
                </h1>
				</div>
                
                <!--SITE TAGLINE-->
        
                {.section siteTagLine}<p>{@}</p>{.end}
        
              {.end}
			  
			  
			  
			  <div class="header_nav_row clearfix">
			  <!--HEADER CTA-->
			  <div class="header_cta">
			  
			  <a href="#" class="getstarted">Getting Started</a><br />
			  
			  
			  Call us (561) 394.9484
			  
			  <div class="arrow_gray">
				  <!-- BEGIN LivePerson Button Code -->
					<span id="lpButDivID-1343765279405"></span>
					<script type="text/javascript" charset="UTF-8" src="https://server.iad.liveperson.net/hc/38344763/?cmd=mTagRepstate&site=38344763&buttonID=7&divID=lpButDivID-1343765279405&bt=3&c=1"></script>
				   <!-- END LivePerson Button code -->
			  </div>
			  </div>
        
              <!--MAIN NAVIGATION-->
       
              <squarespace:navigation navigationId="mainNav" template="navigation" />
			  
			 </div> 
			  
			  
		
		</header>
	</div>
     <!--END HEADER-->
    

    <!-- BEGIN MAIN CONTENT WRAP -->
    
    <div id="maincontent_wrap">
    
            <!-- begin pre-content section-->
            <section id="pre_content">
                    <squarespace:block-field id="preContentBlocks" columns="12" />
                </section>
            <!-- end pre-content section-->
            
        
            <!-- begin main content section -->
            <section id="content">   
			
			
        
			
            
                <!--CONTENT INJECTION POINT--> 
                {squarespace.main-content}
				
				
				
                
            </section>
            <!-- end main content section -->
            
             <!--Begin Post-content--> 
            <section id="post_content_wrap">
                    <div id="post_content">
					
					
					post-content
					
					
					
					</div>
            </section>
            <!--End Post-content-->
     </div>
	<!-- END MAIN CONTENT WRAP -->
    
    <!--Begin Footer-->  
    <div id="footer_wrap">
		<footer class="group" id="footer">
        	footer
        </footer>
    </div>
	<!--End Footer-->
   
 </div>
 <!-- End layout wrapper -->
	
	
	
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="/scripts/jquery-1.7.2.min.js"><\/script>')</script>

